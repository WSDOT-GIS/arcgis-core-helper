{
	"name": "@wsdot/types-arcgis-core-adapter",
	"description": "Provides enhanced type information for $arcgis.import",
	"version": "1.2.0",
	"type": "module",
	"types": "index.d.ts",
	"bin": {
		"arcgis-cdn-dts": "bin/generate-dts.js"
	},
	"files": [
		"dist/**",
		"*.d.ts",
		"bin/**"
	],
	"author": "Jeff Jacobson",
	"license": "Unlicense",
	"scripts": {
		"prepare": "bun tools/generate-dts.ts && bunx --bun biome format index.d.ts --write",
		"prepublishOnly": "bun tools/build.ts",
		"lint": "biome lint",
		"test": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5.8.3",
		"@arcgis/core": "^4.33.9"
	},
	"keywords": [
		"arcgis",
		"type",
		"typing"
	],
	"repository": "github:WSDOT-GIS/types_arcgis-core-adapter",
	"bugs": "https://github.com/WSDOT-GIS/types_arcgis-core-adapter/issues",
	"homepage": "https://github.com/WSDOT-GIS/types_arcgis-core-adapter#readme",
	"packageManager": "bun@1.2.17"
}